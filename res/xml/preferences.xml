<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <CheckBoxPreference android:key="enabled" android:title="@string/enabled"/>
        <ListPreference android:key="which_contacts" android:title="@string/which_contacts"
                android:entries="@array/which_contacts" android:entryValues="@array/which_contacts_values"
                android:defaultValue="all" />
        <ListPreference android:key="delay" android:title="@string/delay"
                android:entries="@array/delay"  android:entryValues="@array/delay_values"
                android:defaultValue="2" />
        <CheckBoxPreference android:key="use_speakerphone" android:title="@string/use_speakerphone"/>
        <CheckBoxPreference android:key="headset_only" android:title="@string/headset_only"/>
        <CheckBoxPreference android:key="no_second_call" android:title="@string/no_second_call"/>
        <com.example.acceptcallproximity.preference.AcceptCallDialogPreference
                android:key="about" android:title="@string/about_title" android:src="@drawable/icon"
                android:dialogTitle="@string/about_title" android:dialogMessage="@string/about_message"
                android:dialogIcon="@drawable/icon" android:negativeButtonText="@null" />
        <com.example.acceptcallproximity.preference.AcceptCallDialogPreference
                android:key="license" android:title="@string/Title"
                android:src="@drawable/icon" android:dialogTitle="@string/Title"
                android:dialogMessage="@string/license_message"
                android:dialogIcon="@drawable/icon" android:negativeButtonText="@null" />
    

</PreferenceScreen>